super + {minus,equal}
    ~/.bin/volume {down,up}

super + shift + {minus,equal}
    ~/.bin/volume {mute,max}

super + P     
    ~/.bin/png_run

super + z     
    ~/.bin/ibus_change

super + g 
    ~/.bin/browser

super + shift + {0,-,=} 
	sudo systemctl {suspend,reboot,poweroff}
    
super + S
	flameshot gui

super + v
	clipmenu

